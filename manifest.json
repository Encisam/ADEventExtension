{
    "manifest_version": 3,
    "name": "ADEventExtension",
    "version": "1.0",
    "description": "Elimina el modal de suscripci√≥n o de anuncios en Chrome.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs",
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "unlimitedStorage",
        "menus.overrideContext"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ]
        }
    ]
}