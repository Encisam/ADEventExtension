{
    "manifest_version": 3,
    "name": "ADEventExtension",
    "version": "1.0",
    "description": "Elimina el modal de suscripci√≥n o de anuncios en Chrome.",
    "permissions": [
        "activeTab",
        "scripting",
        "storage",
        "tabs",
        "webNavigation",
        "webRequest",
        "webRequestBlocking",
        "unlimitedStorage",
        "menus.overrideContext"
    ],
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js": [
                "script.js"
            ]
        }
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "Assets/icon16.png",
            "48": "Assets/icon32.png",
            "128": "Assets/icon128.png"
        }
    }
}